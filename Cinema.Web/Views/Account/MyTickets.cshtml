@model Cinema.Web.Models.MyTicketsViewModel

@{
    ViewBag.Title = "My tickets";
}
<div class="page-header">
    <h1>My tickets</h1>
</div>

<div class="row">
    <div class="col-sm-12">
        <ul class="nav nav-pills">
            <li class="active"><a data-toggle="tab" href="#upcomming">Upcomming seances</a></li>
            <li><a data-toggle="tab" href="#past">Past seances</a></li>
        </ul>
    </div>
</div>
<hr />
<div class="row">
    <div class="col-sm-12">
        <div class="tab-content">
            <div id="upcomming" class="tab-pane fade in active">
                @Html.Partial("TicketList", Model.UpcomingTickets)
            </div>
            <div id="past" class="tab-pane fade">
                @Html.Partial("TicketList", Model.PastTickets)
            </div>
        </div>
    </div>
</div>
<hr/>
<div class="row">
    <div class="col-sm-12">
        @Html.ActionLink("< Back", "MyProfile")
    </div>
</div>